upstream django {
    server unix:///home/thalysonliratl_ln/cursodjango3/blog/mysite.sock; 
}

server {
    listen      80;
    server_name example.com;
    charset     utf-8;

    client_max_body_size 75M; 

    location /media  {
        alias /home/thalysonliratl_ln/cursodjango3/blog/media; 
    }

    location /static {
        alias /home/thalysonliratl_ln/cursodjango3/blog/static;
    }

    location / {
        uwsgi_pass  django;
        include     /home/thalysonliratl_ln/cursodjango3/blog/uwsgi_params; 
    }
}

uwsgi --socket mysite.sock --module blog.wsgi --chmod-socket=666

[uwsgi]
chdir           = /home/thalysonliratl_ln/cursodjango3/blog
module          = blog.wsgi
home            = /home/thalysonliratl_ln/venv
master          = true
processes       = 10
socket          = /home/thalysonliratl_ln/cursodjango3/blog/mysite.sock
vacuum          = true
chmod-socket    = 666